{
    "name": "ngn-template",
    "version": "2.1.1",
    "license": "MIT",
    "scripts": {
        "build": "nx build",
        "format": "prettier \"**/*.{js,ts,html,scss}\" --write -c",
        "lint": "nx run-many --all --target=lint --quiet --fix --cache --cache-location '.angular/cache/.eslintcache'",
        "lint-style": "stylelint **/*.scss --fix --cache --quiet --cache-location '.angular/cache/.stylelintcache'",
        "ng": "nx",
        "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
        "prepare": "husky install",
        "serve:bff": "nx run bff:serve:development",
        "serve:web": "nx run shell:serve:development --devRemotes=auth-web,profile-web",
        "test": "nx run-many --all --target=test --cache"
    },
    "private": false,
    "dependencies": {
        "@angular/animations": "14.1.2",
        "@angular/cdk": "^14.1.2",
        "@angular/common": "14.1.2",
        "@angular/compiler": "14.1.2",
        "@angular/core": "14.1.2",
        "@angular/forms": "14.1.2",
        "@angular/platform-browser": "14.1.2",
        "@angular/platform-browser-dynamic": "14.1.2",
        "@angular/router": "14.1.2",
        "@nestjs/common": "^9.0.9",
        "@nestjs/core": "^9.0.9",
        "@nestjs/mapped-types": "1.1.0",
        "@nestjs/microservices": "^9.0.9",
        "@nestjs/platform-express": "^9.0.9",
        "@nrwl/angular": "14.5.6",
        "kafkajs": "^2.1.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "~7.5.6",
        "tslib": "^2.4.0",
        "zone.js": "~0.11.8"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "14.1.2",
        "@angular-eslint/eslint-plugin": "~14.0.2",
        "@angular-eslint/eslint-plugin-template": "~14.0.2",
        "@angular-eslint/template-parser": "~14.0.2",
        "@angular/cli": "14.1.2",
        "@angular/compiler-cli": "14.1.2",
        "@angular/language-service": "14.1.2",
        "@nestjs/schematics": "^9.0.1",
        "@nestjs/testing": "^9.0.9",
        "@nrwl/cli": "14.5.6",
        "@nrwl/cypress": "14.5.6",
        "@nrwl/eslint-plugin-nx": "14.5.6",
        "@nrwl/jest": "14.5.6",
        "@nrwl/linter": "14.5.6",
        "@nrwl/nest": "14.5.6",
        "@nrwl/node": "14.5.6",
        "@nrwl/workspace": "14.5.6",
        "@types/jest": "28.1.6",
        "@types/node": "18.7.3",
        "@typescript-eslint/eslint-plugin": "~5.33.0",
        "@typescript-eslint/parser": "~5.33.0",
        "autoprefixer": "^10.4.8",
        "cypress": "^10.4.0",
        "eslint": "~8.22.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-cypress": "^2.12.1",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsdoc": "^39.3.6",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "eslint-plugin-sort": "^2.4.0",
        "eslint-plugin-sort-class-members": "^1.14.1",
        "husky": "^8.0.1",
        "jest": "^27.5.1",
        "jest-preset-angular": "^12.2.0",
        "ng-packagr": "~14.1.0",
        "nx": "14.5.6",
        "postcss": "^8.4.16",
        "postcss-import": "~14.1.0",
        "postcss-preset-env": "~7.7.2",
        "postcss-url": "~10.1.3",
        "prettier": "^2.7.1",
        "stylelint": "^14.10.0",
        "stylelint-config-prettier-scss": "^0.0.1",
        "stylelint-config-standard-scss": "^5.0.0",
        "tailwindcss": "^3.1.8",
        "ts-jest": "28.0.7",
        "ts-node": "10.9.1",
        "typescript": "~4.7.4"
    }
}