{
    "name": "ngn-template",
    "version": "2.1.1",
    "license": "MIT",
    "scripts": {
        "build": "nx build",
        "format": "prettier \"**/*.{js,ts,html,scss}\" --write -c",
        "lint": "nx run-many --all --target=lint --quiet --fix --cache --cache-location '.angular/cache/.eslintcache'",
        "lint-style": "stylelint **/*.scss --fix --cache --quiet --cache-location '.angular/cache/.stylelintcache'",
        "ng": "nx",
        "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
        "prepare": "husky install",
        "serve:bff": "nx run bff:serve:development",
        "serve:web": "nx run shell:serve:development --devRemotes=auth-web,profile-web",
        "test": "nx run-many --all --target=test --cache"
    },
    "private": false,
    "dependencies": {
        "@angular/animations": "16.0.0",
        "@angular/cdk": "16.0.0",
        "@angular/common": "16.0.0",
        "@angular/compiler": "16.0.0",
        "@angular/core": "16.0.0",
        "@angular/forms": "16.0.0",
        "@angular/platform-browser": "16.0.0",
        "@angular/platform-browser-dynamic": "16.0.0",
        "@angular/router": "16.0.0",
        "@nestjs/common": "^9.4.0",
        "@nestjs/core": "^9.4.0",
        "@nestjs/mapped-types": "1.2.2",
        "@nestjs/microservices": "^9.4.0",
        "@nestjs/platform-express": "^9.4.0",
        "@ng-web-apis/common": "2.1.0",
        "@nrwl/angular": "16.1.1",
        "kafkajs": "2.2.4",
        "reflect-metadata": "0.1.13",
        "rxjs": "7.8.1",
        "tslib": "2.5.0",
        "zone.js": "0.13.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "16.0.0",
        "@angular-eslint/eslint-plugin": "16.0.1",
        "@angular-eslint/eslint-plugin-template": "16.0.1",
        "@angular-eslint/template-parser": "16.0.1",
        "@angular/cli": "16.0.0",
        "@angular/compiler-cli": "16.0.0",
        "@angular/language-service": "16.0.0",
        "@nestjs/schematics": "^9.1.0",
        "@nestjs/testing": "^9.4.0",
        "@nrwl/cli": "^15.9.3",
        "@nrwl/cypress": "16.1.1",
        "@nrwl/eslint-plugin-nx": "16.1.1",
        "@nrwl/jest": "16.1.1",
        "@nrwl/linter": "16.1.1",
        "@nrwl/nest": "16.1.1",
        "@nrwl/node": "16.1.1",
        "@nrwl/workspace": "16.1.1",
        "@tailwindcss/typography": "^0.5.9",
        "@types/jest": "^29.5.1",
        "@types/node": "20.1.0",
        "@typescript-eslint/eslint-plugin": "^5.59.2",
        "@typescript-eslint/parser": "^5.59.2",
        "autoprefixer": "^10.4.14",
        "cypress": "^12.11.0",
        "eslint": "~8.40.0",
        "eslint-config-prettier": "8.8.0",
        "eslint-plugin-cypress": "^2.13.3",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsdoc": "^44.0.0",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-rxjs": "^5.0.3",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-plugin-sort": "^2.10.0",
        "eslint-plugin-sort-class-members": "^1.17.1",
        "eslint-plugin-unicorn": "^47.0.0",
        "husky": "^8.0.3",
        "jest": "29.5.0",
        "jest-environment-jsdom": "29.5.0",
        "jest-preset-angular": "13.1.0",
        "ng-packagr": "~16.0.0",
        "nx": "16.1.1",
        "postcss": "^8.4.23",
        "postcss-import": "~15.1.0",
        "postcss-preset-env": "~8.3.2",
        "postcss-url": "~10.1.3",
        "prettier": "^2.8.8",
        "stylelint": "^15.6.1",
        "stylelint-config-prettier-scss": "^0.0.1",
        "stylelint-config-standard-scss": "^9.0.0",
        "tailwindcss": "3.3.2",
        "ts-jest": "29.1.0",
        "ts-node": "10.9.1",
        "typescript": "5.0.4"
    }
}
