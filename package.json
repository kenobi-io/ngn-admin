{
    "name": "ngn-template",
    "version": "2.0.3",
    "license": "MIT",
    "scripts": {
        "build": "nx build",
        "format": "prettier \"**/*.{js,ts,html,scss}\" --write -c",
        "lint": "nx run-many --all --target=lint --quiet --fix --cache --cache-location '.angular/cache/.eslintcache'",
        "lint-style": "stylelint **/*.scss --fix --cache --quiet --cache-location '.angular/cache/.stylelintcache'",
        "ng": "nx",
        "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
        "prepare": "husky install",
        "serve:bff": "nx run bff:serve:development",
        "serve:web": "nx run shell:serve:development --devRemotes=auth-web,profile-web",
        "test": "nx run-many --all --target=test --cache"
    },
    "private": false,
    "dependencies": {
        "@angular/animations": "14.0.1",
        "@angular/common": "14.0.1",
        "@angular/compiler": "14.0.1",
        "@angular/core": "14.0.1",
        "@angular/forms": "14.0.1",
        "@angular/platform-browser": "14.0.1",
        "@angular/platform-browser-dynamic": "14.0.1",
        "@angular/router": "14.0.1",
        "@nestjs/common": "^8.4.6",
        "@nestjs/core": "^8.4.6",
        "@nestjs/mapped-types": "1.0.1",
        "@nestjs/microservices": "^8.4.6",
        "@nestjs/platform-express": "^8.4.6",
        "@nrwl/angular": "14.2.4",
        "kafkajs": "^2.0.2",
        "reflect-metadata": "^0.1.13",
        "rxjs": "~7.5.5",
        "tslib": "^2.4.0",
        "zone.js": "~0.11.5"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "14.0.1",
        "@angular-eslint/eslint-plugin": "~13.5.0",
        "@angular-eslint/eslint-plugin-template": "~13.5.0",
        "@angular-eslint/template-parser": "~13.5.0",
        "@angular/cli": "14.0.1",
        "@angular/compiler-cli": "14.0.1",
        "@angular/language-service": "14.0.1",
        "@nestjs/schematics": "^8.0.11",
        "@nestjs/testing": "^8.4.6",
        "@nrwl/cli": "14.2.4",
        "@nrwl/cypress": "14.2.4",
        "@nrwl/eslint-plugin-nx": "14.2.4",
        "@nrwl/jest": "14.2.4",
        "@nrwl/linter": "14.2.4",
        "@nrwl/nest": "14.2.4",
        "@nrwl/node": "14.2.4",
        "@nrwl/workspace": "14.2.4",
        "@types/jest": "28.1.1",
        "@types/node": "17.0.42",
        "@typescript-eslint/eslint-plugin": "~5.27.1",
        "@typescript-eslint/parser": "~5.27.1",
        "cypress": "^10.1.0",
        "eslint": "~8.17.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-cypress": "^2.12.1",
        "husky": "^8.0.1",
        "jest": "^27.5.1",
        "jest-preset-angular": "^12.1.0",
        "nx": "14.2.4",
        "prettier": "^2.6.2",
        "stylelint": "^14.9.1",
        "stylelint-config-prettier-scss": "^0.0.1",
        "stylelint-config-standard-scss": "^4.0.0",
        "ts-jest": "28.0.4",
        "ts-node": "10.8.1",
        "typescript": "~4.7.2"
    }
}